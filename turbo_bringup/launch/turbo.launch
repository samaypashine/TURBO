<?xml version="1.0"?>
<launch>
  <arg name="debug" default="false"/>
  <arg name="limited" default="false"/>
  <arg name="robot_description" default="$(find turbo_description)/launch/load_ur5.launch" doc="Robot description launch file."/>

  <!-- ur5 driver -->
  <!-- Load robot URDF -->
  <arg name="kinematics_config" value="$(find turbo_description)/config/left_arm_calibration.yaml"/> <!-- ?????????????????????????????????????????? -->
  <include file="$(find turbo_description)/launch/load_turbo.launch">
    <arg name="kinematics_params" value="$(arg kinematics_config)"/>
  </include>

  <group ns="left">    
    <include file="$(find ur_robot_driver)/launch/ur5_bringup.launch">
      <arg name="debug" value="$(arg debug)"/>
      <arg name="limited" value="$(arg limited)"/>
      <arg name="robot_ip" value="182.22.22.2"/>
      <arg name="tf_prefix" value="left"/>
      <arg name="kinematics_config" value="$(arg kinematics_config)"/>  
      <arg name="controller_config_file" value="$(find ur_robot_driver)/config/ur5_left_controllers.yaml"/>    
      <arg name="reverse_port" value="50001"/>
      <arg name="script_sender_port" value="50002"/>
      <arg name="trajectory_port" value="50003"/>
      <arg name="script_command_port" value="50004"/>
    </include>
  </group>

  <group ns="right">    
    <include file="$(find ur_robot_driver)/launch/ur5_bringup.launch">
      <arg name="debug" value="$(arg debug)"/>
      <arg name="limited" value="$(arg limited)"/>
      <arg name="robot_ip" value="172.22.22.2"/>
      <arg name="tf_prefix" value="right"/>
      <arg name="kinematics_config" value="$(find turbo_description)/config/right_arm_calibration.yaml"/>
      <arg name="controller_config_file" default="$(find ur_robot_driver)/config/ur5_right_controllers.yaml"/>
      <arg name="reverse_port" value="50005"/>
      <arg name="script_sender_port" value="50006"/>
      <arg name="trajectory_port" value="50007"/>
      <arg name="script_command_port" value="50010"/>
    </include>
  </group>

  <node name="right_joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" respawn="true">
    <rosparam param="source_list">[/right/gripper/joint_states, /right/joint_states]</rosparam>
  </node>
  
  <node name="left_joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" respawn="true">
    <rosparam param="source_list">[/left/gripper/joint_states, /left/joint_states]</rosparam>
  </node>
  
  <!--gripper driver -->
  <!-- <include file="$(find turbo_bringup)/launch/robotiq_85.launch">
    <arg name="gripper_test" value="$(arg gripper_test)"/>
    <arg name="comport" value="/dev/ttyUSB0"/>
  </include> -->

  <!-- Run the main MoveIt executable -->
  <include file="$(find turbo_dual_moveit_config)/launch/move_group.launch">
    <arg name="allow_trajectory_execution" value="true"/>
    <arg name="fake_execution" value="false"/>
    <arg name="info" value="true"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>

  <!-- Run Rviz and load the default config to see the state of the move_group node -->
  <include file="$(find turbo_dual_moveit_config)/launch/moveit_rviz.launch">
    <arg name="rviz_config" value="true"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>

  <!-- Planning Functionality -->
  <include ns="move_group" file="$(find turbo_dual_moveit_config)/launch/planning_pipeline.launch.xml">
    <arg name="pipeline" value="ompl" />
  </include>

</launch>
